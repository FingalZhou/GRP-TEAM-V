                              <ToggleGroup fx:id="secondcompare" />
                           </toggleGroup>
                        </RadioMenuItem>
                        <RadioMenuItem mnemonicParsing="false" text="Quick Sort" toggleGroup="$secondcompare" />
                        <RadioMenuItem mnemonicParsing="false" text="Insertion Sort" toggleGroup="$secondcompare" />
                    </items>
                  </Menu>
               </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Language">
               <items>
                  <MenuItem mnemonicParsing="false" text="C" />
                  <MenuItem mnemonicParsing="false" text="Java" />
                  <MenuItem mnemonicParsing="false" text="C++" />
                  <MenuItem mnemonicParsing="false" text="JavaScript" />
               </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Help">
               <items>
                  <MenuItem mnemonicParsing="false" text="About Domino" />
                  <MenuItem mnemonicParsing="false" text="Domino Help" />
               </items>
            </Menu>
